{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 600,
    "title": "Cancer Incidence and Mortality Rates in Queensland (2009-2011)",
    "projection": {
      "type": "mercator"
    },
    "layer": [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/SlayerPuppets/FIT3179/main/Hospital%20Map/data/suburb-2-qld.topojson",
          "format": {
            "type": "topojson",
            "feature": "ckan_6bedcb55_1b1f_457b_b092_58e88952e9f0.1"  
          }
        },
        "mark": {
          "type": "geoshape",
          "stroke": "white",
          "strokeWidth": 0.5
        },
        "encoding": {
          "color": {
            "field": "Incidence Rate",  
            "type": "quantitative",
            "scale": {
              "scheme": "reds"
            },
            "legend": {"title": "Incidence Rate"}
          },
          "tooltip": [
            {"field": "LGA", "type": "nominal", "title": "Suburb"},
            {"field": "Incidence Rate", "type": "quantitative", "title": "Incidence Rate"},
            {"field": "Mortality Rate", "type": "quantitative", "title": "Mortality Rate"}
          ]
        }
      }
    ],
    "transform": [
      {
        "lookup": "LGA",
        "from": {
          "data": {
            "url": "https://raw.githubusercontent.com/SlayerPuppets/FIT3179/main/Hospital%20Map/dataqueensland_cancer_data.csv",  
            "format": {"type": "csv"}
          },
          "key": "LGA",
          "fields": ["Incidence Rate", "Mortality Rate"]
        }
      }
    ]
  }
  